                        <Tab closable="false" onSelectionChanged="#onSupportInProgress" text="In Progress">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <ListView fx:id="supportInProgressList" layoutX="33.0" layoutY="56.0" onKeyPressed="#onSupportInProgressKey" onMouseClicked="#onSupportInProgressClicked" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
