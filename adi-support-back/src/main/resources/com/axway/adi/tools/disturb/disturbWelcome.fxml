                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab closable="false" onSelectionChanged="#onSupportClosed" text="Closed">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
